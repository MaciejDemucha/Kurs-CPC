@model HashSet<string>


@{ ViewData["Title"] = "Modele"; }

<html>
<head>
    <title>Producenci</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link href="@Url.Content("~/Scripts/Style.css")" rel="stylesheet" type="text/css" />
</head>
<body>
    <!--Nagłówek listy-->
    <h2>Lista producentów</h2>
    <!--Wyświetlenie listy producentów-->
    <ul>
        @foreach (var manufacturer in Model)
        {
<li>@Html.ActionLink(manufacturer, "GetPhoneByManufacturer", "Home", new { model = manufacturer }, null)</li>}
    </ul>

</body>
</html>
